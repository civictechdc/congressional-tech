{
  "$schema": "https://turborepo.com/schema.json",
  "extends": ["//"],
  "tasks": {
    "sync": {
      "outputs": [".venv/**"]
    },
    "makemigrations": {
      "dependsOn": ["sync"],
      "outputs": ["src/committee_meeting/migrations/versions/**"]
    },
    "migrate": {
      "dependsOn": ["sync"],
      "cache": false
    }
  }
}
