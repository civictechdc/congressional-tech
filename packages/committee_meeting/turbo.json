{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "sync": {
      "outputs": [".venv/**"]
    },
    "migrate": {
      "dependsOn": ["^sync", "^makemigrations"],
    },
    "makemigrations": {
      "dependsOn": ["^sync"],
      "outputs": ["src/committee_meeting/migrations/versions/**"]
    }
  },
  "extends": ["//"]
}